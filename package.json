{
  "name": "babel-typescript-sample",
  "version": "0.7.0",
  "license": "MIT",
  "scripts": {
    "preStart": "npm run build",
    "start": "node ./build/app.js",
    "watch": "babel src --watch --out-dir build --source-maps inline --extensions \".ts,.tsx\"",
    "debug": "nodemon --inspect ./build/app.js",
    "build": "babel src --out-dir build --extensions \".ts,.tsx\""
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/polyfill": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0-beta.54",
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.2.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "mongoose": "^5.2.4"
  }
}
