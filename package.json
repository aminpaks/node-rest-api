{
  "name": "babel-typescript-sample",
  "version": "0.7.0",
  "license": "MIT",
  "scripts": {
    "prestart": "npm run build",
    "start": "node ./build/app.js",
    "watch": "babel src --watch --out-dir build --source-maps inline --extensions \".ts,.tsx\"",
    "debug": "nodemon --inspect ./build/app.js",
    "build": "babel src --out-dir build --extensions \".ts,.tsx\""
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.12.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.4",
    "mongoose-plugin-autoinc": "^1.1.9",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0-beta.54",
    "@types/bcrypt": "^2.0.0",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/helmet": "0.0.38",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mongoose": "^5.2.0",
    "@types/morgan": "^1.7.35",
    "typescript": "^2.9.1"
  },
  "engines": {
    "node": "9.11.2"
  }
}
