{
  "name": "rest-api-sample",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node start.js",
    "prestart": "npm run type-check",
    "dev": "npm-run-all \"build -- --source-maps inline\" -p -c debug build:watch",
    "type-check": "tsc",
    "debug": "nodemon --inspect start.js",
    "build": "babel src --out-dir build --extensions \".ts\"",
    "build:watch": "npm run build -- --watch --source-maps inline"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.12.1",
    "jsonwebtoken": "^8.3.0",
    "lodash-es": "^4.17.10",
    "mongoose": "^5.2.4",
    "mongoose-plugin-autoinc": "^1.1.9",
    "morgan": "^1.9.0",
    "pluralize": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "@types/bcrypt": "^2.0.0",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/helmet": "0.0.38",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.112",
    "@types/mongoose": "^5.2.0",
    "@types/morgan": "^1.7.35",
    "npm-run-all": "^4.1.3",
    "typescript": "^2.9.1"
  },
  "engines": {
    "node": "9.11.2"
  }
}
